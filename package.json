{
  "name": "litchi-sea-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "configureJPush": "node node_modules/jpush-react-native/JPushConfiguration.js",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "dev": "node node_modules/react-native/local-cli/cli.js run-android",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios",
    "test": "jest",
    "prettier": "prettier --write --single-quote --no-semi --trailing-comma es5 --print-width 80 \"app/**/*.js\"",
    "lint": "eslint --ext .js app test --fix",
    "format": "yarn prettier && yarn lint -- --fix",
    "precommit": "npm run lint",
    "emu": "C:/Users/DYNAMX/AppData/Local/Android/sdk/tools/emulator.exe -netdelay none -netspeed full -avd android",
    "bundleAndroid": "node node_modules/react-native/local-cli/cli.js bundle --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle --platform android --assets-dest ./android/app/src/main/res/ --dev false",
    "bundleIOS": "react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ./ios/bundle/main.jsbundle --assets-dest ./ios/bundle",
    "key": "keytool -genkey -alias demo.keystore -keyalg RSA -validity 40000 -keystore demo.keystore",
    "listIOSDevice": "xcrun simctl list devices",
    "listAndroidDevice": "adb devices",
    "IOSX": "react-native run-ios --simulator 'iPhone X'",
    "IOS6": "react-native run-ios --simulator 'iPhone 6'",
    "IOS6P": "react-native run-ios --simulator 'iPhone 6 Plus'",
    "IOSDeviceDebug": "react-native run-ios --device 'yourPhoneName' --configuration Debug",
    "IOSDeviceRelease": "react-native run-ios --device 'yourPhoneName' --configuration Release",
    "AndroidDeviceDebug": "cd android && ./gradlew installDebug",
    "AndroidDeviceRelease": "cd android && ./gradlew installRelease"
  },
  "dependencies": {
    "antd-mobile": "^2.1.11",
    "axios": "^0.17.1",
    "crypto-js": "^3.1.9-1",
    "dva-core": "^1.1.0",
    "jcore-react-native": "^1.2.2",
    "jpush-react-native": "^2.1.3",
    "net": "^1.0.2",
    "node-forge": "^0.7.1",
    "rc-form": "^2.1.7",
    "react": "^16.2.0",
    "react-native": "^0.51.1",
    "react-native-camera": "1.0.2",
    "react-native-htmlview": "^0.12.1",
    "react-native-image-crop-picker": "^0.19.0",
    "react-native-qq": "^2.0.5",
    "react-native-qrcode-scanner": "0.0.30",
    "react-native-router-flux": "^4.0.0-beta.27",
    "react-native-vector-icons": "^4.5.0",
    "react-native-wechat": "^1.9.10",
    "react-navigation": "^1.0.0-beta.23",
    "react-redux": "^5.0.6",
    "teaset": "^0.5.5"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "^21.2.0",
    "babel-plugin-import": "^1.6.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-react-native": "^4.0.0",
    "eslint": "^4.13.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.3.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "prettier": "^1.9.1",
    "react-test-renderer": "^16.2.0",
    "flow-bin": "^0.62.0",
    "prop-types": "^15.6.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
